#ifndef OPENCLX_BUILD_STATUS
#define OPENCLX_BUILD_STATUS

#include <openclx/forward>

namespace clx {

    enum class build_status: build_status_type {
        success = CL_BUILD_SUCCESS,
        none = CL_BUILD_NONE,
        error = CL_BUILD_ERROR,
        in_progress = CL_BUILD_IN_PROGRESS,
    };

}

#endif // vim:filetype=cpp
